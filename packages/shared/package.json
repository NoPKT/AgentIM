{
  "name": "@agentim/shared",
  "version": "0.1.0",
  "description": "Shared types, protocol, validators and i18n for AgentIM",
  "license": "AGPL-3.0-only",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/NoPKT/AgentIM.git",
    "directory": "packages/shared"
  },
  "author": "NoPKT LLC",
  "engines": {
    "node": ">=20"
  },
  "keywords": [
    "agentim",
    "shared",
    "protocol",
    "types"
  ],
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./i18n": {
      "types": "./dist/i18n/index.d.ts",
      "import": "./dist/i18n/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist .turbo",
    "lint": "tsc --noEmit",
    "test": "node --import tsx --test test/*.test.ts"
  },
  "dependencies": {
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "tsx": "^4.21.0",
    "typescript": "^5.8.0"
  }
}
