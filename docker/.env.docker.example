# AgentIM Docker Configuration
# Copy this file to .env and fill in the values, or use deploy.sh to auto-generate.

# Required â€” generate with: openssl rand -base64 32
JWT_SECRET=
ENCRYPTION_KEY=

# Admin credentials (password must be 8+ chars with upper, lower, and digit)
ADMIN_USERNAME=admin
ADMIN_PASSWORD=

# PostgreSQL password (internal, not exposed to host)
POSTGRES_PASSWORD=

# Optional overrides
PORT=3000
CORS_ORIGIN=http://localhost:3000
JWT_ACCESS_EXPIRY=15m
JWT_REFRESH_EXPIRY=7d
